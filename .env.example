# PostgreSQL Configuration
POSTGRES_DB=overplanned
POSTGRES_USER=overplanned
POSTGRES_PASSWORD=your_secure_postgres_password_here

# PgBouncer Configuration
PGBOUNCER_POOL_MODE=transaction
PGBOUNCER_MAX_CLIENT_CONN=100
PGBOUNCER_DEFAULT_POOL_SIZE=25
PGBOUNCER_MIN_POOL_SIZE=5
PGBOUNCER_RESERVE_POOL_SIZE=5
PGBOUNCER_SERVER_IDLE_TIMEOUT=600

# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password_here
REDIS_MAXMEMORY=256mb

# Qdrant Configuration
QDRANT_API_KEY=your_secure_qdrant_api_key_here

# Database URLs (for application use)
# Direct PostgreSQL connection
DATABASE_URL=postgresql://overplanned:your_secure_postgres_password_here@localhost:15432/overplanned

# PgBouncer pooled connection (recommended for app)
DATABASE_POOLED_URL=postgresql://overplanned:your_secure_postgres_password_here@localhost:16432/overplanned

# Redis connection
REDIS_URL=redis://:your_secure_redis_password_here@localhost:16379

# Qdrant connection
QDRANT_URL=http://localhost:6333
QDRANT_GRPC_URL=http://localhost:6334

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_min_32_chars_here

# Google OAuth
GOOGLE_CLIENT_ID=your_google_oauth_client_id
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret

# Stripe (beta mode - not enforcing payments)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# Resend Email
RESEND_API_KEY=re_your_resend_api_key

# Sentry Error Tracking
SENTRY_DSN=https://your_sentry_dsn
NEXT_PUBLIC_SENTRY_DSN=https://your_sentry_dsn

# Anthropic API
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key

# Environment
NODE_ENV=development

# FastAPI
NEXT_PUBLIC_API_URL=http://localhost:8000
